// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  charId           String     @default(cuid()) @id
  level            String
  power            String
  buyPrice         String
  equippedItems    String[]
  equippedGold     String
  url              String
}

